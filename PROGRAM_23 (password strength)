#include<stdio.h>
#include<string.h>
#include<conio.h>
int main()
{
    char pass[100];
    int i;
    printf("enter the password");
    scanf("%s",pass);
    int l=strlen(pass);
    int up=0,lw=0,dg=0,sp=0;
    if(l>8)
    {
        for(int i=0;i<l;i++)
        {
            if(isupper(pass[i]))
                up++;
            else if(islower(pass[i]))
                lw++;
            else if(isdigit(pass[i]))
                dg++;
            else
                sp++;
        }
        if(up>0 && lw>0 && dg>0 && sp>0)
            printf("strong password");
        else
            printf("weak password");
    }
    else
        printf("wrong password due to less no of characters");
}

#include<stdio.h>
int main()
{
  int i,a;
  printf("enter any number:");
  scanf("%d",&a);
  if(a%2==0)
  {
      for(i=1;i<=10;i++)
      {
      printf("%d * %d = %d",a,i,a*i);
      printf("\n");
      }
  }
  else
  {
      for(i=1;i<=15;i++)
      {
      printf("%d * %d = %d",a,i,a*i);
      printf("\n");
      }
  }
}

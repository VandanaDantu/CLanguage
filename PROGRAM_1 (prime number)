#include <stdio.h>
int main()
{
    int a,count=0,i;
    printf("enter any number:");
    scanf("%d",&a);
    for(i=2;i<a;i++)
    {
        if(a%i==0)
        {
            count++;
        }
    }
    if(count==0)
    {
        printf("prime number");
    }
    else
        printf("composite number");
}

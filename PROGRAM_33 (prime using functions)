#include <stdio.h>
int prime(int n)
{
    int count=0;
    for(int i=2;i<n;i++)
    {
        if(n%i==0)
        {
            count++;
        }
    }
    if(count==0)
       printf("it is prime");
    else
       printf("it is a composite");
}
int main()
{
    int a;
    printf("Enter any number:");
    scanf("%d",&a);
    prime(a);
    return 0;
}
